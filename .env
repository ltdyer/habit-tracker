# this .env file is just for the docker-compose files to be able to interpolate values at compile time
# neither the Backend container nor the Frontend container uses the values here
FRONTEND_PORT = 3000
BACKEND_PORT = 3001
FRONTEND_PORT_PROD = 3002
BACKEND_PORT_PROD = 3003

# mongoDB: these env variables make sure the mongo instance is run with auth enabled
MONGO_INIT_DB_ROOT_USERNAME = root
MONGO_INIT_DB_ROOT_PASSWORD = password

# mongo express: UI variables
MONGO_EXPRESS_PORT = 8081
ME_CONFIG_MONGODB_ADMINUSERNAME = root
ME_CONFIG_MONGODB_ADMINPASSWORD = password
ME_CONFIG_MONGODB_URL = mongodb://root:password@mongo:27017/